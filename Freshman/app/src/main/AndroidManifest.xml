            android:resizeableActivity="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".UI.Activity.HomePageActivity"
            android:launchMode="singleTop"
            android:resizeableActivity="false"
            android:screenOrientation="portrait">

        </activity>
        <activity
            android:name=".UI.Activity.CampusStrategyActivity"
            android:launchMode="singleTop"
            android:resizeableActivity="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".UI.Activity.MienActivity"
            android:launchMode="singleTop"
            android:resizeableActivity="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".UI.Activity.OnlineActivity"
            android:launchMode="singleTop"
            android:resizeableActivity="false"
            android:screenOrientation="portrait" />
        <activity android:name=".UI.Activity.IWannaSayActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
